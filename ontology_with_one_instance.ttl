epd:hasDataGenerator a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "has Data Generator",
        "has Data Generator"@en ;
    elements:description "This property connects AdministrativeInformation to DataGenerator, identifying the person(s), working group(s), organization(s), or database network responsible for generating the EPD data set and ensuring its correctness regarding methods, inventory, and documentation. This information is crucial for data provenance and quality assurance." ;
    rdfs:comment "Associates administrative information with the entity responsible for generating or modelling the data set" ;
    rdfs:domain epd:AdministrativeInformation ;
    rdfs:range epd:DataGenerator ;
    owl:inverseOf epd:isDataGeneratorOf .

