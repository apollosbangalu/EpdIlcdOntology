epd:ADPE a epd:EnvironmentalImpactIndicators,
        owl:NamedIndividual ;
    rdfs:label "Abiotic depletion potential nonfossil resources (ADPE)"@en ;
    elements:description "Abiotic Depletion Potential - Non-Fossil Resources (ADPE) assesses the potential for a substance to deplete non-renewable resources other than fossil fuels. The unit for ADPE is typically \"kg Sb-eq\" (kilograms of antimony equivalent)." ;
    rdfs:comment "Abiotic depletion potential - non-fossil resources" .

epd:ADPF a epd:EnvironmentalImpactIndicators,
        owl:NamedIndividual ;
    rdfs:label "Abiotic depletion potential fossil resources (ADPF)"@en ;
    elements:description "Abiotic Depletion Potential - Fossil Resources (ADPF) measures the potential for a substance to deplete fossil fuel resources. The unit for ADPF is typically \"MJ\"." ;
    rdfs:comment "Abiotic depletion potential - fossil resources" .

epd:AP a epd:EnvironmentalImpactIndicators,
        owl:NamedIndividual ;
    rdfs:label "Acidification potential Accumulated Exceedance (AP)" ;
    elements:description "Acidification Potential, Accumulated Exceedance (AP) denotes the potential for a substance to contribute to acid rain or the acidification of ecosystems when it surpasses certain thresholds. The unit for AP is typically \"mol H+ eq\"." ;
    rdfs:comment "Acidification potential, Accumulated Exceedance" .

epd:AccessAndUseRestrictions a owl:DatatypeProperty ;
    rdfs:label "Access and Use Restrictions" ;
    elements:description "Access restrictions / use conditions for this data set as free text or referring to e.g. license conditions. In case of no restrictions \"None\" is entered." ;
    rdfs:comment "The access and use restrictions for the data set" ;
    rdfs:domain epd:PublicationAndOwnership ;
    rdfs:range xsd:string .

epd:ApprovalOfOverallComplianceStatus a owl:DatatypeProperty ;
    rdfs:label "Approval of Overall Compliance Status"@en ;
    rdfs:comment "the Approval of overall compliance status"@en ;
    rdfs:domain epd:ComplianceDeclarations ;
    rdfs:range xsd:string .

epd:BiogenicCarbonContent a owl:DatatypeProperty ;
    rdfs:label "has Biogenic Carbon Content",
        "Biogenic Carbon Content"@en ;
    elements:description "This property provides information about the biogenic carbon content in the quantitative reference flow of the product. Biogenic carbon refers to carbon derived from renewable biological sources, which plays an important role in carbon footprint calculations and climate change impact assessments. The value is typically expressed as a percentage or absolute amount of biogenic carbon present in the declared unit." ;
    rdfs:comment "Specifies the biogenic carbon content of the reference flow" ;
    rdfs:domain epd:QuantitativeReference ;
    rdfs:range xsd:string .

epd:CRU a epd:OutputIndicatorsDescribingWasteCategories,
        owl:NamedIndividual ;
    rdfs:label "Components for reuse (CRU)"@en ;
    elements:description "Components for Re-Use (CRU) are materials or components that can be reused in their existing form. There is no specific unit for components for re-use." ;
    rdfs:comment "Components for re-use" .

epd:CTUe a epd:Unit,
        owl:NamedIndividual ;
    rdfs:label "CTUe" ;
    rdfs:comment "Comparative Toxic Unit for ecosystems" .

epd:CTUh a epd:Unit,
        owl:NamedIndividual ;
    rdfs:label "CTUh" ;
    rdfs:comment "Comparative Toxic Unit for humans" .

