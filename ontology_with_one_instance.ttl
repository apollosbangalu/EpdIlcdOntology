    rdfs:label "Use B1" ;
    rdfs:comment "Use phase (B1)" .

epd:Uuid a owl:DatatypeProperty ;
    rdfs:label "Uuid"@en ;
    elements:description "Automatically generated universally unique identifier of this data set. Together with the \"Data set version\", the UUID uniquely identifies each data set. Find further explanations in FAQ." ;
    rdfs:comment "This is the Universal Unique Identification"@en ;
    rdfs:domain epd:KeyDataSetInformation ;
    rdfs:range xsd:string .

epd:Value a owl:DatatypeProperty ;
