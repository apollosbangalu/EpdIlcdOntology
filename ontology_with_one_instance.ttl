epd:ProcessDataSet a owl:Class ;
    rdfs:label "Process Data Set" ;
    rdfs:comment "A data set for a specific process of a building material or product" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass epd:AdministrativeInformation ;
            owl:onProperty epd:hasAdministrativeInformation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass epd:EnvironmentalIndicator ;
            owl:onProperty epd:hasEnvironmentalIndicator ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass epd:ModellingAndValidation ;
            owl:onProperty epd:hasModellingAndValidation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass epd:ProcessInformation ;
            owl:onProperty epd:hasProcessInformation ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

