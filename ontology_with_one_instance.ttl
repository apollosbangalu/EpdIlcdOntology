epd:hasAdditionalEnvironmentalImpactIndicators a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "has Additional Environmental Impact Indicators",
        "has Additional Environmental Impact Indicators"@en ;
    elements:description "This property connects EnvironmentalImpactIndicators to AdditionalEnvironmentalImpactIndicators, which represent supplementary environmental impact categories beyond the core mandatory indicators. These may include toxicity indicators, land use impacts, ionizing radiation, and other specialized impact categories that provide a more comprehensive environmental assessment." ;
    rdfs:comment "Associates core environmental impact indicators with additional supplementary impact indicators" ;
    rdfs:domain epd:EnvironmentalImpactIndicators ;
    rdfs:range epd:AdditionalEnvironmentalImpactIndicators ;
    owl:inverseOf epd:isAdditionalEnvironmentalImpactIndicatorsOf .

